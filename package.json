{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel frontend_start backend_start",
    "frontend_start": "cd frontend && npm start",
    "backend_start": "cd server && npm run dev",
    "build": "npm-run-all --parallel frontend_build backend_build",
    "frontend_build": "cd frontend && npm run build",
    "backend_build": "cd server && npm run build"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
